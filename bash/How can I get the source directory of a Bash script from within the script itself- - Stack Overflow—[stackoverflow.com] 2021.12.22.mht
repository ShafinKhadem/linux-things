amp;&amp; <span class=3D"hljs-built_in">pwd</span> -P )
    <span class=3D"hljs-built_in">echo</span> Script-Dir : $(dirname <span =
class=3D"hljs-string">"<span class=3D"hljs-subst">$(readlink -f <span class=
=3D"hljs-string">"<span class=3D"hljs-variable">$0</span>"</span>)</span>"<=
/span>)
